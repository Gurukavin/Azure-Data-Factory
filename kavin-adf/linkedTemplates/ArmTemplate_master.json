{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "kavin-adf"
		},
		"Blobsource_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Blobsource'"
		},
		"Incremental_raw_blob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Incremental_raw_blob'"
		},
		"sink_sql_server_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sink_sql_server'"
		},
		"httpgit_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://raw.githubusercontent.com/"
		},
		"sink_sql_server_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "kavinsql.database.windows.net"
		},
		"sink_sql_server_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "kavin_db"
		},
		"sink_sql_server_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Adminkavin"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "kavin-adf_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"Blobsource_connectionString": {
						"value": "[parameters('Blobsource_connectionString')]"
					},
					"Incremental_raw_blob_connectionString": {
						"value": "[parameters('Incremental_raw_blob_connectionString')]"
					},
					"sink_sql_server_password": {
						"value": "[parameters('sink_sql_server_password')]"
					},
					"httpgit_properties_typeProperties_url": {
						"value": "[parameters('httpgit_properties_typeProperties_url')]"
					},
					"sink_sql_server_properties_typeProperties_server": {
						"value": "[parameters('sink_sql_server_properties_typeProperties_server')]"
					},
					"sink_sql_server_properties_typeProperties_database": {
						"value": "[parameters('sink_sql_server_properties_typeProperties_database')]"
					},
					"sink_sql_server_properties_typeProperties_userName": {
						"value": "[parameters('sink_sql_server_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}